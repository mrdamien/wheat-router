<?xml version="1.0" encoding="UTF-8"?>
<router>
    <path pattern="team">
        <path pattern="john" id="john">
            <return code="301" location="team/1"  />
        </path>
        <path pattern="jane">
            <return code="200" file="jane.php"  />
        </path>
        <path name="team_id" pattern="\d+" id="team.id">
            <return file="{0}.php" />
        </path>
        <return code="200" location="roster.php"  />
    </path>
    <path pattern="lost">
        <path name="name" pattern="(?&lt;id&gt;\d+)">
            <return code="302" location="/team/{id}"  />
        </path>
    </path>
</router>
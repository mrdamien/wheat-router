<?xml version="1.0" encoding="UTF-8"?>
<router>
    <path pattern="team">
        <path name="team_id" pattern="(.+)" id="team.id">
            <render file="{0}.php" />
        </path>
        <render file="roster.php" />
    </path>
    <path pattern="lost">
        <path name="name" pattern="(?&lt;team_id&gt;\d+)">
            <http code="302" route="team.id" team_id="{team_id}" />
        </path>
    </path>
</router>